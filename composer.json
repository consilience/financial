{
    "name": "consilience/iso8583",
    "description": "A simple PHP ISO8583 pack and unpack library",
    "license": "MIT",
    "require": {
        "php": "~7.0",
        "zeptech/annotations": "^1.1",
        "tightenco/collect": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Consilience\\Iso8583\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Consilience\\Iso8583\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "code-style": "./vendor/bin/phpcs --standard=build/phpcs.xml src",
        "code-style-fix": "./vendor/bin/phpbf src"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
